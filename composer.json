{
    "name": "vendor/money-statistics",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require": {
        "php": "^8.3",
        "symfony/console": "^7.1",
        "symfony/runtime": "^7.1",
        "symfony/dotenv": "^7.1",
        "php-di/php-di": "^7.0",
        "symfony/finder": "^7.1",
        "league/csv": "^9.18",
        "moneyphp/money": "^3.3",
        "brick/money": "^0.10.0",
        "ext-intl": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpstan/phpstan": "^2.0",
        "symplify/easy-coding-standard": "^12.3"
    },
    "scripts": {
        "phpstan": "phpstan analyse -c phpstan.neon",
        "standards": "vendor/bin/ecs check",
        "standards-fix": "vendor/bin/ecs check --fix",
        "check-all": [
            "@standards",
            "@phpstan"
        ]
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    }
}
